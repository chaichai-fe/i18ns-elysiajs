{
    "name": "elysiajs-i18n",
    "version": "0.0.1",
    "description": "Translation API built with ElysiaJS",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "dev": "bun run --hot src/index.ts",
        "build": "bun build src/index.ts --outdir=dist --target=bun",
        "build:exe": "bun build --compile --minify-whitespace --minify-syntax --outfile server src/index.ts",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:push": "drizzle-kit push",
        "start": "./server",
        "deploy": "./deploy.sh"
    },
    "dependencies": {
        "@elysiajs/bearer": "^1.1.4",
        "@elysiajs/cors": "^1.1.1",
        "@elysiajs/jwt": "^1.1.1",
        "@elysiajs/openapi": "^1.3.11",
        "@elysiajs/swagger": "^1.1.5",
        "@planetscale/database": "^1.19.0",
        "dotenv": "^17.2.2",
        "drizzle-orm": "^0.44.5",
        "elysia": "^1.1.22",
        "mysql2": "^3.14.5"
    },
    "devDependencies": {
        "bun-types": "latest",
        "drizzle-kit": "^0.31.4",
        "tsx": "^4.20.5"
    }
}